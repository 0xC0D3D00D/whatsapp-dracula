{
  "name": "whatsup-dracula",
  "title": "Whatsup Dracula",
  "version": "0.1",
  "description": "Dracula theme for Whatsup web",
  "license": "CC-BY-SA-4.0",
  "repository": "0xc0d3d00d/whatsup-dracula",
  "homepage": "https://github.com/0xc0d3d00d/whatsup-dracula",
  "main": "whatsup-dracula.user.css",
  "engines": {
    "node": ">=10"
  },
  "devDependencies": {
    "eslint": "^7.0.0",
    "eslint-config-silverwind": "^13.3.3",
    "perfectionist": "^2.4.0",
    "stylelint": "^13.5.0",
    "stylelint-config-standard": "^20.0.0",
    "updates": "^10.2.12",
    "versions": "8.2.10"
  },
  "scripts": {
    "authors": "bash tools/authors",
    "clean": "npm run perfectionist && node tools/fix-perfectionist.js",
    "eslint": "eslint tools/*.js",
    "lint": "npm run eslint && npm run stylelint",
    "major": "versions -C -p major whatsup-dracula.user.css",
    "minor": "versions -C -p minor whatsup-dracula.user.css",
    "patch": "versions -C -p patch whatsup-dracula.user.css",
    "perfectionist": "perfectionist whatsup-dracula.user.css whatsup-dracula.css --indentSize 2 --maxAtRuleLength 250",
    "stylelint": "stylelint whatsup-dracula.user.css",
    "test": "npm run eslint && npm run stylelint",
    "update": "updates -cu && npm install"
  }
}
